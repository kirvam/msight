create table if not exists req_note ( 
id mediumint not null auto_increment, 
req_n_id VARCHAR(20) NOT NULL, 
req_n_tag VARCHAR(20) NOT NULL,
req_n_type VARCHAR(30),
req_n_note VARCHAR(50) not null, 
primary key (id) );

create table if not exists req_type (
id mediumint not null auto_increment,
req_t_tag VARCHAR(20) NOT NULL,
req_t_type VARCHAR(30) NOT NULL,
primary key (id) );

